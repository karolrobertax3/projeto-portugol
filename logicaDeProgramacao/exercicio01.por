programa {
	
	
	funcao inicio() 
	
	{
		
		
	inteiro dias, meses, anos,totaldias		
			
	escreva("Digite o total de anos: ")
		
			
	leia(anos)
		
			
	escreva("Digite total de meses: ")
		
		
	leia(meses)
		
			
	escreva("Total de dias: ")
		
		
	leia(dias)
		
		
	totaldias = 365 * anos + 30 * meses + dias
		
	escreva("Você tem ", totaldias, " dias de vida")
	
	}	


}